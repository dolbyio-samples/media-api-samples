							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{dolby-io-url}}/media/jobs",
							"host": [
								"{{dolby-io-url}}"
							],
							"path": [
								"media",
								"jobs"
							],
							"query": [
								{
									"key": "submitted_after",
									"value": null,
									"description": "Query jobs that were submitted at or after the specified date and time (inclusive).",
									"disabled": true
								},
								{
									"key": "submitted_before",
									"value": null,
									"description": "Query jobs that were submitted at or before the specified date and time (inclusive). The submitted_before must be the same or later than submitted_after.",
									"disabled": true
								},
								{
									"key": "status",
									"value": null,
									"description": "Query jobs that have the specified status.",
									"disabled": true
								},
								{
									"key": "next_token",
									"value": null,
									"description": "Used when querying the next page of jobs. Specify the next_token that was returned in the previous call.",
									"disabled": true
								}
